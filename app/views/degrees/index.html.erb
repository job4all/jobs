
<center><h3>Degrees</h3></center>
<hr class="colorgraph">
<table class="table">
  <thead>
    <tr>
      <th>Degree Type</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @degrees.each do |degree| %>
      <tr>
        <td><%= degree.tdegree.name %></td>
        <td><%= degree.name %></td>
        <td><%= link_to 'Show', degree %></td>
        <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
        <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
    

<br>
<hr class="colorgraph">
<%= link_to 'New Degree', new_degree_path , :class => "btn btn-info"%>
