

<center><h3>Education Details</h3></center>
<hr class="colorgraph">
<table class="table">
  <thead>
    <tr>
      <th>Degree Type</th>
      <th>Degree</th> 
      <th>Subject</th>
      <th>City</th>
      <th>Institute</th>
      <th>Tmarks</th>
      <th>Omarks</th>
      <th>Persent</th>
      <th>Division</th>
      <th>Passyear</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.tdegree.name %></td>
        <td><%= education.degree.name %></td>
        <td><%= education.subject %></td>
        <td><%= education.city.name %></td>
        <td><%= education.institute.name %></td>
        <td><%= education.tmarks %></td>
        <td><%= education.omarks %></td>
        <td><%= education.persent %></td>
        <td><%= education.division %></td>
        <td><%= education.passyear %></td>
        <td><%= link_to '', edit_education_path(education),class: "glyphicon glyphicon-pencil" %></td>
        <td><%= link_to '', education, method: :delete, data: { confirm: 'Are you sure?' } ,class: "glyphicon glyphicon-trash" %></td> 
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<hr class="colorgraph">
<%= link_to 'New Education', new_education_path , :class => "btn btn-info"%> 
