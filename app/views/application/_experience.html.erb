<h4>Experiences</h4>
<hr class="colorgraph">
<table class="table">
  <thead>
    <tr>
      <th>Designation</th>
      <th>Profession</th>
      <th>City</th>
      <th>Organization</th>
      <th>Sdate</th>
      <th>Endyear</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
     <% @user.experiences.each do |experience| %>
      <tr>
        <td><%= experience.designation %></td>
        <td><%= experience.profession.name %></td>
        <td><%= experience.city.name %></td>
        <td><%= experience.organization.name %></td>
        <td><%= experience.start_date.strftime("%d-%m-%Y")%></td>
        <td><%= experience.end_date.strftime("%d-%m-%Y") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<hr>
